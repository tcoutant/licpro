struct Element
{
	int valeur;
	int rang;//facultatif
	struct Element *suivant;
};
typedef struct Element *Liste;
/* retourner la drniere occurence d'une valeur*/
void retourner(Liste l,int val)
{
Liste dern;
dern=NULL;
while (l !=NULL)
	{
	if (l->valeur== val)
	dern=l;
	l=l-> suivant;
	}
return dern;
