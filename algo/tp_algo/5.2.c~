# include <stdio.h>
# include <string.h>
# define L 20 		/*declaration des constantes*/

/*commentaire*/


int un_a_un(char *S)
{
int i=0;
while(S[i]!='\n') i++;
return i;
}

int main()			
{
char S[L];
printf("ecrivez une chaine de caractere pour vous donner en reponse sa longueur\n");
fgets(S,L-1,stdin);
printf("%d\n",un_a_un(S));
return 0;
}

		

