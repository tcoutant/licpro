# include <stdio.h>
# include <string.h>
# define const 10 		/*declaration des constantes*/


/*prototype de la fonction F (declaration)*/
float F(int);

/*definition de la fonction*/			
 float F(int k)			
{
float f;
for(f=1;k>0;k--)
	f=f*k;
return f;
}

/*programme principal*/
void main()
{
printf("la factorielle de 5 est %f . \n",F(5));

} 
