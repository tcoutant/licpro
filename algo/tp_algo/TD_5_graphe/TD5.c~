#include <stdlib.h>
#include <stdio.h>

#define n 6

int GRAPHE[n][n];
int M[n];

struct Element
{
	 int val;
	 struct Element * suivant;
};
typedef struct Element * File;

void prof(int s,int g[n][n], int M[n])
{
int i;
printf("%d ",s);
M[s-1]=1;
	for (i=1;i<=n;i++)
		if ((M[i-1]==0) && (g[s-1][i-1]==1))
			prof(i,g,M);
}
