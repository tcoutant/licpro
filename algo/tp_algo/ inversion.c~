# include <stdio.h>
#define size 6
int temp,i, tab[size];
void main() {
for(i=0;i<size;i++)
{
printf ("donner un nombre :\n");
scanf ("%d", &tab[i]);
}
temp=tab[6];	
for(i=0;i<size-1;i++)
{	
tab[i+1]=tab[i];
}
tab[1]=temp;
//ecriture du tableau
for(i=0;i<size;i++)
{
printf ("    %d  ",tab[i]);	
}
}nb
